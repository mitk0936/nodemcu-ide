{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^build", "@nodemcu-ide/nodemcu-tool-binary#build"],
      "cache": false,
      "persistent": true
    }
  }
}
